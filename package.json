{
  "name": "feathers-google-maps",
  "version": "1.0.0",
  "description": "A Feathers service for interacting with the Google Maps API",
  "keywords": [
    "feathers",
    "google maps"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": ">= 16.0.0"
  },
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "src/**",
    "dist/**"
  ],
  "scripts": {
    "build": "unbuild",
    "version": "npm run build",
    "release": "np",
    "vitest": "vitest",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DaddyWarbucks/feathers-google-maps.git"
  },
  "author": "DaddyWarbucks",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DaddyWarbucks/feathers-google-maps/issues"
  },
  "homepage": "https://github.com/DaddyWarbucks/feathers-google-maps#readme",
  "dependencies": {
    "@feathersjs/errors": "^5.0.1",
    "@feathersjs/feathers": "^5.0.1",
    "@googlemaps/google-maps-services-js": "^3.3.28"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "@vitest/coverage-c8": "^0.29.8",
    "cross-env": "^7.0.3",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unicorn": "^46.0.0",
    "np": "^7.7.0",
    "prettier": "^2.8.7",
    "typescript": "^5.0.3",
    "unbuild": "^1.2.0",
    "vite": "^4.2.1",
    "vitest": "^0.29.8"
  }
}
